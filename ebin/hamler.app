  {env, []},
  {licenses, ["BSD 3-Clause"]},
  {maintainers, ["Feng Lee <feng@emqx.io>",
                 "Yang Miao <yangm@emqx.io>"]},
  {links, [{"Homepage", "https://hamler.io/"},
