import Test.System.Dir as Dir
import Test.Data.Bits as Bits
import Test.Data.Char as Char
import Test.System.Env as Env
import Test.System.File as File

main :: IO ()
main = runTest $ TxG "lib" 
   [ TxG "Data" [
                   TxG "Map"      [M.test]
                 , TxG "String"   [S.test]
                 , TxG "Atom"     [A.test]
                 , TxG "Array"    [A1.test]
                 , TxG "Bool"     [TB.test]
                 , TxG "List"     [L.test]
                 , TxG "Bool"     [TB.test]
                 , TxG "Either"   [Ei.test]
                 , TxG "Tuple"    [Tu.test]
                 , TxG "Maybe"    [May.test]
                 , TxG "OrdDict"  [OD.test]
                 , TxG "OrdSets"  [OS.test]
                 , TxG "Queue"    [Q.test]
                 , TxG "Result"   [R.test]
                 , TxG "Set"      [St.test]
                 , TxG "Tuple"    [Tu.test]
                 , TxG "Show"     [Show.test]
                 , TxG "Monoid"   [Monoid.test]
                 , TxG "Graph"    [G.test]
                 , TxG "Ord"      [Ord.test]
                 , TxG "Function" [Func.test]
                 , TxG "Semiring" [SR.test]
                 , TxG "Ring"     [Ring.test]
                 , TxG "Timeout"  [Timeout.test]
                 , TxG "Ordering" [Ordering.test]
                 , TxG "Functor"  [Functor.test]
                 , TxG "Term"     [TxG "Persistent" [P.test]]
                 , TxG "Regex" [Re.test]
                 , TxG "Bits"  [Bits.test]
                 , TxG "Char"  [Char.test]
                 ],
     TxG "System" [ TxG "Dir"     [Dir.test]
                  , TxG "Env"     [Env.test]
                  , TxG "File"    [File.test]
                  ]
   ]

